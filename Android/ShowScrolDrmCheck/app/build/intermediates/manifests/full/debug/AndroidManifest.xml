<?xml version="1.0" encoding="utf-8"?>
<!-- android:sharedUserId="android.uid.system" -->
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hisense.checkdrm"
    android:sharedUserId="android.uid.system"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.hisense.checkdrm.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name="com.hisense.receiver.BootCompletedReceiver" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>

        <activity android:name="com.hisense.checkdrm.ShowActivity" />
        <activity android:name="com.hisense.checkdrm.ResultActivity" />
    </application>

</manifest>